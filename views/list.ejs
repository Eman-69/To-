<%-include("header")-%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <%litem.forEach(function(i){%>
            <form action="/delete" method="post"><div class="item">
                <input name="checkbox" value=<%=i._id%> onChange="this.form.submit()" type="checkbox" name="" id="">
                <p><%=i.name%></p></div></form>
            <%console.log(i.name)})%>
            <form class="item" action="/" method="post">
                <input type="text" name="newItem" id="todo" required placeholder="todo">
                <button type="submit" name="list" value=<%=listTitle %>>+</button>
            </form>
</div>
<%-include("footer")-%>

</body>
</html>